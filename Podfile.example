# Uncomment the next line to define a global platform for your project
platform :ios, '14.0'

# Comment the next line if you don't want to use dynamic frameworks
use_frameworks!

# Comment the next line if you're not using Swift and don't want to use dynamic frameworks
use_modular_headers!

target 'YourAppTarget' do
  # WBMAnalytics - Full library
  pod 'WBMAnalytics', '~> 3.4.3'

  target 'YourAppTargetTests' do
    inherit! :search_paths
    # Pods for testing
    pod 'WBMAnalytics/Tests', '~> 3.4.3'
  end

  target 'YourAppTargetUITests' do
    # Pods for testing
  end
end

# Post install hook to configure build settings
post_install do |installer|
  installer.pods_project.targets.each do |target|
    target.build_configurations.each do |config|
      config.build_settings['IPHONEOS_DEPLOYMENT_TARGET'] = '14.0'
      config.build_settings['SWIFT_VERSION'] = '5.7'
    end
  end
end 